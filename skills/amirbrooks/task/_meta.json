{
  "owner": "amirbrooks",                           // 技能所有者：amirbrooks（GitHub 用户名）
  "slug": "task",                                 // 技能标识符：task（用于内部引用）
  "displayName": "任务",                          // 显示名称：任务（在界面中显示的中文名称）
  "latest": {                                     // 最新版本信息
    "version": "0.1.0",                           // 当前版本号：0.1.0（语义化版本号）
    "publishedAt": 1769123617767,                 // 发布时间：2026-01-26 03:00:17（Unix 时间戳，毫秒）

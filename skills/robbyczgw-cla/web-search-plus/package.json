{
  "name": "@clawdbot/web-search-plus",
  "version": "2.0.0",
  "description": "Unified search skill with Smart Auto-Routing. Automatically selects between Serper (Google), Tavily (Research), and Exa (Neural) based on query analysis. No provider selection needed - just search! Fully configurable routing keywords and priorities.",
  "keywords": [
    "clawdbot",
    "skill",
    "search",
    "serper",
    "tavily",
    "exa",
    "google",
    "research",
    "semantic-search",
    "web-search",
    "ai-agent",
    "auto-routing",
    "smart-routing",
    "multi-provider"
  ],
  "author": "Robby <robbyczgw@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robbyczgw-cla/web-search-plus.git"
  },
  "homepage": "https://clawdhub.com/robbyczgw-cla/web-search-plus",
  "bugs": {
    "url": "https://github.com/robbyczgw-cla/web-search-plus/issues"
  },
  "clawdbot": {
    "skill": true,
    "triggers": ["search", "find", "look up", "research"],
    "capabilities": [
      "web-search",
      "image-search",
      "semantic-search",
      "multi-provider"
    ],
    "providers": ["serper", "tavily", "exa"]
  },
  "files": [
    "SKILL.md",
    "README.md",
    "scripts/",
    ".env.example"
  ]
}
